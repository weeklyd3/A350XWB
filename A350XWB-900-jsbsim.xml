        <input>/systems/fbw/aileron</input>
        <!--<input>/systems/fbw/</input>-->
        <clipto>
          <min>-1</min>
          <max>1</max>
        </clipto>
      </summer>
      <!--<summer name="Left Aileron Sum">
        <input>/systems/fbw/aileron</input>
        <input>/systems/fbw/symmetric-aileron</input>
        <output>fcs/left-aileron-sum</output>
        <clipto>
          <max>1</max>
          <min>-1</min>
        </clipto>
      </summer>-->
      <aerosurface_scale name="Left Aileron Control">
        <input>fcs/left-aileron-sum</input>
        <gain>/systems/hydraulic/both-gain</gain>
        <range>
          <min>-15</min>
          <max>15</max>
        </range>
        <output>fcs/left-aileron-pos-rad</output>
      </aerosurface_scale>
      <!--<summer name="Right Aileron Sum">
        <input>-/systems/fbw/aileron</input>
        <input>/systems/fbw/symmetric-aileron</input>
        <output>fcs/right-aileron-sum</output>
        <clipto>
          <max>1</max>
          <min>-1</min>
        </clipto>
      </summer>-->
      <aerosurface_scale name="Right Aileron Control">
        <input>fcs/right-aileron-sum</input>
        <gain>/systems/hydraulic/both-gain</gain>
        <range>
          <min>-15</min>
          <max>15</max>
        </range>
        <output>fcs/right-aileron-pos-rad</output>
      </aerosurface_scale>
      <aerosurface_scale name="Left Aileron position normalized">
        <input>fcs/left-aileron-pos-deg</input>
        <domain>
          <min>-17.18873385</min>
          <max>17.18873385</max>
        </domain>
        <range>
          <min>-1</min>
          <max>1</max>
        </range>
        <output>fcs/left-aileron-pos-norm</output>
      </aerosurface_scale>
      <aerosurface_scale name="Right Aileron position normalized">
        <input>fcs/right-aileron-pos-deg</input>
        <domain>
          <min>-17.18873385</min>
          <max>17.18873385</max>
        </domain>
        <range>
          <min>-1</min>
          <max>1</max>
        </range>
        <output>fcs/right-aileron-pos-norm</output>
      </aerosurface_scale>
    </channel>
    <channel name="Yaw">
      <summer name="Rudder Command Sum">
        <input>fcs/rudder-cmd-norm</input>
        <input>fcs/yaw-trim-cmd-norm</input>
        <clipto>
          <min> -0.35 </min>
          <max>  0.35 </max>
        </clipto>
      </summer>
      <scheduled_gain name="Yaw Damper Rate">
        <input>velocities/r-aero-rad_sec</input>
        <table>
          <independentVar lookup="row">velocities/ve-kts</independentVar>
          <tableData>
