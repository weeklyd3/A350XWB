<text>
    <name>fcu_spdmachtext</name>
    <offsets>
        <pitch-deg>0</pitch-deg>
        <heading-deg>0</heading-deg>
        <roll-deg>0</roll-deg>
        <x-m>2.94041</x-m>
        <y-m>-0.093411</y-m>
