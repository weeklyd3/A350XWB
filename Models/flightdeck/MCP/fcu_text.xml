<PropertyList>
<animation>
 <type>material</type> 
 <object-name>spdmachtext</object-name>
 <object-name>hdgtext</object-name>
 <object-name>alttext</object-name>
 <object-name>vstext</object-name>
 <emission>	
  <red>1</red>	
  <green>1</green>	
  <blue>1</blue>
 </emission>
</animation>
<!-- Must be enclosed by a <text/> node -->
<text>
    <name>spdmachtext</name>
    <offsets>
        <pitch-deg>0</pitch-deg>
        <heading-deg>0</heading-deg>
        <roll-deg>0</roll-deg>
        <x-m>2.94041</x-m>
        <y-m>-0.093411</y-m>
        <z-m>1.60393</z-m>
    </offsets>
    <axis-alignment>yz-plane</axis-alignment>
    <type type="string">literal</type>
    <text type="string">SPD</text>
    <layout>left-to-right</layout>
    <draw-text type="bool">true</draw-text> <!-- draw the text itself -->
    <draw-alignment type="bool">false</draw-alignment> <!-- draw crosshair at object center -->
    <draw-boundingbox type="bool">false</draw-boundingbox> <!-- draw a bounding box -->
 
    <font>LiberationFonts/LiberationSans-Regular.ttf</font> <!-- The font file name, relative to data/Fonts -->
    <character-size type="double">0.005</character-size> <!-- size (height) im meters -->
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <max-height>0.006</max-height> <!-- the maximum height of the text -->
    <max-width>1</max-width> <!-- the maximum width of the text -->
    <font-resolution>
        <width type="int">32</width>
        <height type="int">32</height>
    </font-resolution>
    <alignment>center-top</alignment>
</text>
